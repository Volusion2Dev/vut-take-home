  "engines": {
    "node": ">=12.14.1"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.30.0",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "next": "^12.1.0",
    "pg-promise": "^10.5.8",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.14.2",
    "@testing-library/jest-dom": "^5.11.1",
    "@testing-library/react": "^10.4.7",
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.5",
    "@types/node": "^14.0.23",
    "@types/pg-promise": "^5.4.3",
    "@types/react": "^16.9.43",
    "@types/styled-components": "^5.1.1",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-styled-components": "^1.12.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "jest": "^26.1.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.2",
    "typescript": "^4.3.2"
  },
  "scripts": {
    "audit:ci": "npx audit-ci --high",
    "build": "next build && tsc --project tsconfig.server.json",
    "compile": "tsc --pretty",
    "compile:watch": "tsc --pretty --watch",
    "curl": "curl http://localhost:3000/blocks --verbose",
    "dev": "nodemon",
    "docker": "docker-compose down && docker-compose up",
    "docker:clean": "docker stop full-stack-postgres && docker rm -v full-stack-postgres",
    "lint": "eslint 'src/**/*.ts?(x)'",
    "lint:fix": "npm run lint -- --fix",
    "psql": "psql postgres://postgres:postgres@localhost:5432/postgres",
    "psql:blocks": "npm run psql -- -c 'SELECT id, block_type, position, JSONB_PRETTY(configured_data) as configured_data, created_on, modified_on FROM site_builder.block ORDER BY position;'",
    "start": "next start",
    "test": "jest",
    "test:api": "jest --config jest.server.config.js",
    "test:watch": "jest --watch"
  }
}
